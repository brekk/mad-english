import IO from "IO"

import { Regex, UnflaggedRegex } from "./Regex"
import Sx from "./Stemmer"



main = () => {
  IO.putLine("Hello World!")
  pipe(
    UnflaggedRegex,
    IO.trace("reggie watts"),
  )(`[^aeiou]`)
  pipe(
    Sx.dealWithLeadingY,
    IO.trace("ahoy!"),
  )(Sx.dealWithLeadingY("Yvette"))
}
